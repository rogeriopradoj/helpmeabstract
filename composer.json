{
  "name": "kayladaniels/helpmeabstract",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/kayladnls/php-github-api"
    }
  ],
  "require": {
    "slim/slim": "dev-master",
    "vlucas/phpdotenv": "~1.1@dev",
    "twig/twig": "~2.0@dev",
    "guzzlehttp/guzzle": "^6.0.0",
    "robmorgan/phinx": "dev-master",
    "vlucas/spot2": "dev-master",
    "mailgun/mailgun-php": "^2.0",
    "php-http/guzzle6-adapter": "^1.0"
  },
  "authors": [
    {
      "name": "Kayla Daniels",
      "email": "kayladnls@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": { "Kayladnls\\": "src/" }
  },
  "minimum-stability": "stable",
  "scripts": {
    "run": [
      "@composer install",
      "php -S localhost:8000 -t public public/index.php"
    ]
  }
}
